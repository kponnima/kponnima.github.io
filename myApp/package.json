  "scripts": {
    "ng": "ng",
    "version": "ng version",
    "update": "ng update",
    "start": "npm start",
    "start-server": "node ./bin/www",
    "serve": "ng serve -o",
    "serve-proxy": "ng serve -o --proxy-config proxy.config.json",
    "build": "ng build --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run myApp:server",
