{
  "name": "myapp",
  "version": "1.0.0",
  "engines": {
    "node": "12.3.1",
    "npm": "6.10.1"
  },
  "scripts": {
    "ng": "ng",
    "version": "ng version",
    "update": "ng update",
    "start": "node app.js",
    "serve": "ng serve -o",
    "serve-test": "rimraf dist/* && ng build --watch --progress=true",
    "build": "ng build --prod",
    "build-test": "lite-server",
    "dev": "concurrently \"npm run mongo-win\" \"npm run start\" \"npm run serve\"",
    "test": "ng test",
    "coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "tslint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "e2e-noupdate": "ng e2e --no-webdriver-update",
    "driver-update": "webdriver-manager update",
    "driver-update-noSSL": "webdriver-manager update --ignore_ssl",
    "driver-version": "webdriver-manager status",
    "driver-clean": "webdriver-manager clean",
    "mongo-win": "md mongo-db & \"C:/Program Files/MongoDB/Server/4.0/bin/mongod.exe\" --auth --port 27017 --dbpath mongo-db",
    "mongo-win-noauth": "md mongo-db & \"C:/Program Files/MongoDB/Server/4.0/bin/mongod.exe\" --port 27017 --dbpath mongo-db"
  },
  "private": true,
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^9.0.0-next.3",
    "@angular/cdk": "^8.1.1",
    "@angular/cli": "^8.3.0",
    "@angular/common": "^9.0.0-next.3",
    "@angular/compiler": "^9.0.0-next.3",
    "@angular/compiler-cli": "^9.0.0-next.3",
    "@angular/core": "^9.0.0-next.3",
    "@angular/forms": "^9.0.0-next.3",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^9.0.0-next.3",
    "@angular/platform-browser-dynamic": "^9.0.0-next.3",
    "@angular/router": "^9.0.0-next.3",
    "async": "^3.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "config": "^3.2.2",
    "core-js": "^3.1.4",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "helmet": "^3.20.0",
    "http-errors": "^1.7.3",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "mongoose": "^5.6.6",
