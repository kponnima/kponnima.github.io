{
  "name": "myapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "version": "ng version",
    "update": "ng update",
    "start": "node app.js",
    "start-server": "nodemon app",
    "serve": "ng serve -o",
    "serve-proxy": "ng serve -o --proxy-config proxy.config.json",
    "build": "ng build --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run myApp:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "tslint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "e2e-noupdate": "ng e2e --no-webdriver-update",
    "driver-update": "webdriver-manager update",
    "driver-update-noSSL": "webdriver-manager update --ignore_ssl",
    "driver-version": "webdriver-manager status",
    "driver-clean": "webdriver-manager clean",
    "mongo-win": "md mongo-db & \"C:/Program Files/MongoDB/Server/4.0/bin/mongod.exe\" --auth --port 27017 --dbpath mongo-db",
    "mongo-win-noauth": "md mongo-db & \"C:/Program Files/MongoDB/Server/4.0/bin/mongod.exe\" --port 27017 --dbpath mongo-db"
  },
  "private": true,
  "keywords": [],
  "author": "",
