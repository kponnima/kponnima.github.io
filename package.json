  "nyc": {
    "require": [
      "./test/helpers.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": true,
    "include": [
      "application/**/*.js"
    ],
    "exclude": [
      "application/models/*.js",
      "application/e2e/**/*.js",
      "application/src/**/*.scss"
    ],
    "report-dir": "reports/coverage",
    "temp-dir": "reports/coverage"
  }
